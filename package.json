    "@types/json-schema": "^7.0.12",
    "@types/ms": "^0.7.30",
    "@types/object-hash": "^3.0.2",
    "@types/ramda": "^0.29.3",
    "@types/slug": "^5.0.4",
    "rimraf": "^2.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
