    "@ournet/horoscopes-data": "~0.1.4",
    "@ournet/horoscopes-domain": "~0.1.4",
    "@ournet/images-data": "~0.x.x",
    "@ournet/images-domain": "~0.x.x",
    "@ournet/news-data": "~0.1.8",
